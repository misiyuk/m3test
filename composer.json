{
    "type": "symfony-bundle",
    "name": "misiyuk/math-bundle",
    "description": "Math bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Misiyuk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Misiyuk\\Bundle\\MathBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Misiyuk\\Bundle\\MathBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
